# Ines
class countdown_iterator:
    def __init__(self, count):
        self.count = count
        self.end_count = 0

    def __iter__(self):
        return self

    def __next__(self):
        if self.count >= self.end_count:
            temp = self.count
            self.count -= 1
            return temp
        raise StopIteration()
        
